{
  "name": "@wagmi/solid",
  "description": "Solid Signals for Ethereum",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wevm/wagmi.git",
    "directory": "packages/solid"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && npx tsc",
    "watch": "npx tsc --watch",
    "test": "jest"
  },
  "jest": {
    "preset": "solid-jest/preset/browser",
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/@testing-library/jest-dom"
    ]
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "/actions",
    "/chains",
    "/codegen",
    "/connectors",
    "/query"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/exports/index.js",
  "types": "./dist/types/exports/index.d.ts",
  "typings": "./dist/types/exports/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/exports/index.d.ts",
      "default": "./dist/exports/index.js"
    },
    "./actions": {
      "types": "./dist/types/exports/actions.d.ts",
      "default": "./dist/exports/actions.js"
    },
    "./chains": {
      "types": "./dist/types/exports/chains.d.ts",
      "default": "./dist/exports/chains.js"
    },
    "./codegen": {
      "types": "./dist/types/exports/codegen.d.ts",
      "default": "./dist/exports/codegen.js"
    },
    "./connectors": {
      "types": "./dist/types/exports/connectors.d.ts",
      "default": "./dist/exports/connectors.js"
    },
    "./query": {
      "types": "./dist/types/exports/query.d.ts",
      "default": "./dist/exports/query.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "actions": [
        "./dist/types/exports/actions.d.ts"
      ],
      "chains": [
        "./dist/types/exports/chains.d.ts"
      ],
      "codegen": [
        "./dist/types/exports/codegen.d.ts"
      ],
      "connectors": [
        "./dist/types/exports/connectors.d.ts"
      ],
      "query": [
        "./dist/types/exports/query.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "@tanstack/solid-query": ">=5.0.0",
    "solid-js": ">=1",
    "typescript": ">=5.0.4",
    "viem": "2.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@wagmi/connectors": "4.1.12",
    "@wagmi/core": "2.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@solidjs/testing-library": "^0.8.5",
    "@tanstack/solid-query": "5.18.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@types/jest": "^29.5.12",
    "@types/testing-library__jest-dom": "^5.14.9",
    "babel-jest": "^29.7.0",
    "babel-preset-jest": "^29.6.3",
    "babel-preset-solid": "^1.8.9",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.2.0",
    "regenerator-runtime": "0.14.1",
    "solid-devtools": "^0.29.2",
    "solid-jest": "^0.2.0",
    "solid-js": "1.8.12",
    "solid-testing-library": "^0.5.1",
    "typescript": "^5.2.2"
  },
  "contributors": [
    "awkweb.eth <t@wevm.dev>",
    "jxom.eth <j@wevm.dev>",
    "glitch-txs"
  ],
  "funding": "https://github.com/sponsors/wevm",
  "keywords": [
    "wagmi",
    "react",
    "hooks",
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3"
  ]
}
